                        if (city == '珠海' || desc == 'Zhuhai') 
						{ areaJS = 'http://www.sina.com.cn/js/67/sinaindex/2013/zhuhai.js'; }
                        if (city == '深圳' || desc == 'Shenzhen') 
						{ areaJS = 'http://www.sina.com.cn/js/67/sinaindex/2013/shenzhen.js'; }
                        break;

                    case '福建':
						areaJS = 'http://www.sina.com.cn/js/67/sinaindex/2013/fj.js';
