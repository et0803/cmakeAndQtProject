if(IndexNum == 0){
EduAdSpan1 = 0,EduAdSpan2 = 0,EduAdSpan3 = 0;
}
else if(IndexNum == 1){
EduAdSpan0 = 0,EduAdSpan2 = 0,EduAdSpan3 = 0;
