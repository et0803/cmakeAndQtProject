    <!-- 高考志愿通调用js begin -->
    <script type="text/javascript">
    jsLoader({
        name: 'shm',
        callback: function() {
            SHM.register('home.gaokao', function($) {
                $.io.jsonp('http://zhiyuan.edu.sina.com.cn/api/?p=api&s=api&a=count_stat','',function(data){
                    if(data.status==0){
                        $.dom.byId('ceping_count').innerHTML = data.data['1501'];
                        $.dom.byId('report_count').innerHTML = data.data['1502'];
                        $.dom.byId('query_count').innerHTML = parseInt(data.data['1503']) + parseInt(data.data['1504']) + parseInt(data.data['1505']) + parseInt(data.data['1506']);
